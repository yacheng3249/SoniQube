generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Song {
  id     Int     @id @default(autoincrement())
  name   String
  artist String
  cover  String
  active Boolean @default(false)
  audio  String
}

model User {
  id       Int     @id @default(autoincrement())
  name     String
  email    String
  password String
}